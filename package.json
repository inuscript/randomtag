{
  "name": "randomtag",
  "version": "1.0.0",
  "description": "",
  "main": "random.js",
  "scripts": {
    "start": "static .",
    "build": "npm run build:script ; npm run build:tag",
    "build:tag": "env LC_COLLATE=C sort tags.txt -i -u -o tags.txt",
    "build:script": "browserify -e src/index.js -o build/index.js",
    "build:watch": "watchify -d -v -e src/index.js -o build/index.js",
    "budo": "budo src/index.js:build/index.js",
    "build:script:rollup": "rollup -c",
    "fixup": "npm run build; git add tags.txt; git c -m 'autofix'; git push",
    "diff-check": "git diff --exit-code",
    "test": "npm run build",
    "auto-pr": "node script/auto-pr.js",
    "test:check": "npm run test ; npm run diff-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inuscript/randomtag.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/inuscript/randomtag/issues"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "homepage": "https://github.com/inuscript/randomtag#readme",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.1.1",
    "browserify": "^13.0.0",
    "budo": "^8.0.3",
    "github-basic": "^6.0.0",
    "rollup": "^0.25.2",
    "rollup-plugin-babel": "^2.3.9",
    "rollup-plugin-commonjs": "^2.2.1",
    "rollup-plugin-npm": "^1.4.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "axios": "^0.9.1",
    "clipboard": "^1.5.5",
    "doc-ready": "^1.0.3",
    "vidom": "^0.1.3"
  }
}
