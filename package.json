{
  "name": "randomtag",
  "version": "1.0.0",
  "description": "",
  "main": "random.js",
  "scripts": {
    "start": "static .",
    "build": "build:script ; build:tag",
    "build:tag": "env LC_COLLATE=C sort tags.txt -i -u -o tags.txt",
    "build:script": "rollup -c",
    "fixup": "npm run build; git add tags.txt; git c -m 'autofix'; git push",
    "diff-check": "git diff --exit-code",
    "test": "npm run build",
    "auto-pr": "node script/auto-pr.js",
    "test:check": "npm run test ; npm run diff-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inuscript/randomtag.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/inuscript/randomtag/issues"
  },
  "babel": {
    "presets": ["es2015-rollup"]
  },
  "homepage": "https://github.com/inuscript/randomtag#readme",
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.1.1",
    "github-basic": "^6.0.0",
    "rollup": "^0.25.2",
    "rollup-plugin-babel": "^2.3.9"
  }
}
